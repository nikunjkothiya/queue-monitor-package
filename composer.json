{
    "name": "nikunjkothiya/laravel-queue-monitor",
    "description": "Self-hosted queue failure monitoring and analytics dashboard for Laravel applications.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "queue",
        "monitor",
        "failed-jobs",
        "dashboard",
        "analytics"
    ],
    "authors": [
        {
            "name": "Nikunj Kothiya",
            "email": "nikunjkothiya401@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "illuminate/notifications": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "NikunjKothiya\\QueueMonitor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NikunjKothiya\\QueueMonitor\\Providers\\QueueMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}


