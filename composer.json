{
    "name": "nikunjkothiya/laravel-queue-monitor",
    "description": "Self-hosted queue failure monitoring and analytics dashboard for Laravel applications.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikunj Kothiya",
            "email": "nikunjkothiya401@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/queue": "^11.0|^12.0",
        "illuminate/notifications": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "illuminate/view": "^11.0|^12.0",
        "illuminate/cache": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "NikunjKothiya\\QueueMonitor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NikunjKothiya\\QueueMonitor\\Providers\\QueueMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}


